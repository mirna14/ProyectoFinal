{
  "name": "proyectofinal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": " npm run build && PORT=3000 APP_URL='http://localhost:' DEBUG=proyectofinal node ./dist/bin/www",
    "dev": "PORT=3000 APP_URL='http://localhost:' DEBUG=proyectofinal babel-watch --watch ./server/views ./server/bin/www",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel ./server --out-dir dist --copy-files"
  },
  "dependencies": {
    "babel-preset-minify": "^0.5.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-watch": "^7.7.2"
  }
}
