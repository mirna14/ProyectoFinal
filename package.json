{
  "name": "proyectofinal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": " npm run build && PORT=3000 APP_URL='http://localhost:' DEBUG=proyectofinal node ./dist/bin/www",
    "dev": "PORT=3000 APP_URL='http://localhost:' DEBUG=proyectofinal babel-watch --watch ./server/views ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "build": "npm run buildc && npm run builds",
    "builds": "npm run clean && babel ./server --out-dir dist --copy-files",
    "buildc": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "clean": "rm -rf ./dist",
    "cleanc": "rm -f ./public/bundle.js"
  },
  "dependencies": {
    "babel-preset-minify": "^0.5.2",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.31.0",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-hot-middleware": "^2.25.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-watch": "^7.7.2"
  }
}
